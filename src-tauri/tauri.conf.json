{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:21420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "scrcpy-partner-win",
    "version": "0.3.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "path": {
        "all": true
      },

      "shell": {
        "all": true,
        "scope": [{
          "name": "adb",
          "cmd": "adb",
          "args": true
        },{
          "name": "scrcpy",
          "cmd": "scrcpy",
          "args": true
        }, {
          "name": "scrcpy-control",
          "cmd": "scrcpy-control",
          "args": true
        }]
      },
      "dialog": {
        "all": true, 
        "open": true,
        "save": true
      },
      "window": {
        "all": true
      },
      "process": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "scrcpy.partner.win",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
      ],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      
      {
        "width": 400,
        "height": 200,
        "decorations": false,
        "url": "splashscreen.html",
        "label": "splashscreen",
        "title": "Scrcpy Partner Splashscreen",
        "center": true,
        "resizable": false
        
      }
      
    ]
    
  }
}
